!-----URxvt settings-----!
URxvt.perl-ext-common:default,matcher,clipboard,url-select,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 2
URxvt*buffered: true
URxvt.scrollBar: false
URxvt*cursorBlink: false
URxvt*depth: 32
URxvt*borderless: 1
URxvt*loginShell: true
Urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
URxvt*saveLines: 5000
URxvt.intensityStyles: false
URxvt*underlineColor: #777777
!URxvt.letterSpace: -1

!-----Keybindings-----!

URxvt.keysym.C-minus: perl:font-size:decrease
URxvt.keysym.C-equal: perl:font-size:increase

! get cut/paste to work
URxvt.iso14755: false
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

URxvt.keysym.M-s: perl:keyboard-select:activate
URxvt.keysym.M-r: perl:keyboard-select:search

! make ctrl-arrow work
URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

! disable drawing glyphs
URxvt*skipBuiltinGlyphs:    true

!-- Xft settings -- !
Xft.dpi:        112
Xft.autohint:   0
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault

! -- Fonts -- !
URxvt*font: -*-tamsyn-medium-r-*-*-14-*-*-*-*-*-*-*
URxvt*boldFont: -*-tamsyn-bold-r-*-*-14-*-*-*-*-*-*-*
!URxvt*italicFont:  xft:Bitstream Vera Sans Mono:italic:autohint=true:pixelsize=12
!URxvt*boldItalicFont:  xft:Bitstream Vera Sans Mono:bold:italic:autohint=true:pixelsize=12
URxvt.scrollWithBuffer: true
URxvt.scrollTtyOutput: false
URxvt.scrollTtyKeypress: true

!-----Rofi launcher config-----!
rofi.auto-select:       false
rofi.bc:                #8c9440
rofi.bw:                2
rofi.case-sensitive:    false
rofi.combi-modi:        window,run
rofi.disable-history:   false
rofi.filter:
rofi.fixed-num-lines:   true
rofi.font:              Source Code Pro 10
rofi.hide-scrollbar:    true
rofi.hmode:             false
rofi.lazy-filter-limit: 15000
rofi.levenshtein-sort:  false
rofi.line-margin:       2
rofi.lines:             8
rofi.location:          0
rofi.m:                 -1
rofi.markup-rows:       true
rofi.modi:              window,run,ssh
rofi.opacity:           100
rofi.padding:           12
rofi.parse-hosts:       false
rofi.parse-known-hosts: true
rofi.run-command:       {cmd}
rofi.run-list-command:
rofi.run-shell-command: {terminal} -e {cmd}
rofi.separator-style:   none
rofi.sidebar-mode:      true
rofi.ssh-client:        ssh
rofi.ssh-command:       {terminal} -e {ssh-client} {host}
rofi.ssh-set-title:     true
rofi.terminal:          /usr/bin/termite
rofi.tokenize:          true
rofi.width:             30

!-----Xterm settings-----!
xterm*faceName:-*-tamsyn-medium-r-*-*-14-*-*-*-*-*-*-*
!xterm*faceSize:10


